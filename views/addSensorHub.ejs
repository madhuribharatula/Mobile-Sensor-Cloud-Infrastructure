                                    <div class="control-group">
                                        <label class="control-label">Sensor Hub Name :</label>
                                        <div class="controls">
                                            <input type="text" class="span11" placeholder="Sensor Hub Name"  required name="sensorhubName"/>
                                        </div>
                                    </div>
                                    <div class="control-group">
                                        <label class="control-label">Sensor Hub Location :</label>
                                        <div class="controls">
                                            <input type="text" class="span11" placeholder="Sensor Hub Location"  required name="sensorhubLocation"/>
                                        </div>
                                    </div>

                                    <div class="form-actions">
                                        <input type="submit" class="btn btn-success" value="Add Sensor Hub"></input>

                                    </div>
                                </form>
                            </div>
                        </div>
